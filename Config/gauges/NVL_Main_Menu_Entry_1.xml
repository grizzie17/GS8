<?xml version="1.0"?>
<SimBase.Document Type="AceXML" version="1,0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Gauges.xsd">
	<SimGauge.Gauge id="button-NVL_Screen_Mode">
		<Size>130,20</Size>
		<Update>
			<Script>
				// NOTE: See "NVL_Main_Menu_Item.XML" for register definitions
				(U:Main Menu Ptr) s7
				0x000000 s1 both
				"width" s2 both
				"height" s3 both
				0x00ffff s4 both
			</Script>
		</Update>

		<Element>
			<Include File="NVL_Main_Menu_Item" />
		</Element>
		<MouseArea id="MouseArea">
			<Visible>
				TRUE
			</Visible>
			<FloatPosition>0.000,0.000</FloatPosition>
			<Size>130,20</Size>
			<MouseClick id="MouseClick">
				<KeyEvent></KeyEvent>
			</MouseClick>
		</MouseArea>
	</SimGauge.Gauge>
</SimBase.Document>
