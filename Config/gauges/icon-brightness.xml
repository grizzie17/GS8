<?xml version="1.0"?>
<Gauge Name="icon-brightness" Version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Gauges.xsd">
	<Size>70,45</Size>
	<Element Context="background">
		<!--
			All icon files utlize the registers to pass color information. The current register assignment is as follows:
			r0 defines the visiblity of the icon
			r1..r5 defines the color palet (It is not required to utlize all of the registers)
			-->
		<Visible>r0</Visible>
		<Circle X="20" Y="22" Radius="12" LineColor="r1" FillColor="r1" LineWidth="2"/>
		<Line Color="r1" LineWidth="2">
			<Point X="20" Y="2"/>
			<Point X="20" Y="9"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="0" Y="22"/>
			<Point X="7" Y="22"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="33" Y="22"/>
			<Point X="40" Y="22"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="20" Y="38"/>
			<Point X="20" Y="42"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="10" Y="12"/>
			<Point X="5" Y="8"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="30" Y="12"/>
			<Point X="35" Y="8"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="10" Y="32"/>
			<Point X="5" Y="37"/>
		</Line>
		<Line Color="r1" LineWidth="2">
			<Point X="30" Y="32"/>
			<Point X="35" Y="37"/>
		</Line>
	</Element>
</Gauge>
