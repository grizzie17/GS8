<?xml version="1.0"?>
<Gauge Name="icon-selector-arrow-up" Version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Gauges.xsd">
	<Size>40,40</Size>
	<Element Context="background">
		<!--
			All icon files utlize the registers to pass color information. The current register assignment is as follows:
			r0 defines the visiblity of the icon
			r1..r5 defines the color palet (It is not required to utlize all of the registers)
			-->
		<Visible>r0</Visible>
		<Polygon LineColor="r1" FillColor="r2">
			<Point X="0" Y="20"/>
			<Point X="35" Y="10"/>
			<Point X="35" Y="30"/>
			<Point X="0" Y="20"/>
		</Polygon>
	</Element>
</Gauge>
