<?xml version="1.0"?>
<SimBase.Document Type="AceXML" version="1,0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Gauges.xsd">
	<SimGauge.Gauge id="button-adjust-up">
		<Update>
			<Script>
				0x004000 0x00FF00 (S:MILLI) (U:UP Timeout) &gt; ? s2
				0x00FF00 s1
				both
				(U:BLACKOUT SELECTED) ! (U:ADJUST ACTIVE) AND s0
				both
			</Script>
		</Update>
		<Size>70,40</Size>
		<Element Context="background">
			<Include File="icon-up-arrow" />
		</Element>
	<MouseArea id="MouseArea">
		<FloatPosition>0.000,0.000</FloatPosition>
		<Size>70,40</Size>
		<MouseClick id="MouseClick">
			<KeyEvent>E:UP SELECTED</KeyEvent>
		</MouseClick>
	</MouseArea>
	</SimGauge.Gauge>
</SimBase.Document>
