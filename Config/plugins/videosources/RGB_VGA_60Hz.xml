<?xml version="1.0" encoding="UTF-8"?>
<!-- Plugin information -->
<PluginInclude
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../plugin.xsd">

	<!-- Parameter set for VideoSourceAnalogVdds -->
	<Parameters>

		<!-- The following should be defined in "Plugin" tag
			Video Input Port
			ServerID 
		-->

		<!--VGA 640x480 @ 60Hz progressive 16 bit operation-->
		<Param Name="Height" Type="INTEGER">480</Param>
		<Param Name="Width" Type="INTEGER">640</Param>
		<Param Name="Video Data Mode Flags" Type="ENUM">NO_MODS</Param>
		<Param Name="Capture Profile" Type="ENUM">CAP_VP_CUSTOM</Param>
		<Param Name="Capture Profile ID" Type="ENUM">PROGRESSIVE</Param>
		<Param Name="Capture Mode" Type="ENUM">VPORT_MODE_YCBCR_8BIT</Param>
		<Param Name="Field Frame Op" Type="ENUM">VPORT_FLDOP_PROGRESSIVE</Param>
		<Param Name="X Start Field 1" Type="INTEGER">155</Param>
		<Param Name="Y Start Field 1" Type="INTEGER">2</Param>
		<Param Name="X Start Field 2" Type="INTEGER">0</Param>
		<Param Name="Y Start Field 2" Type="INTEGER">0</Param>
		<Param Name="X Stop Field 1" Type="INTEGER">794</Param>
		<Param Name="Y Stop Field 1" Type="INTEGER">479</Param>
		<Param Name="X Stop Field 2" Type="INTEGER">0</Param>
		<Param Name="Y Stop Field 2" Type="INTEGER">0</Param>
		<Param Name="Threshold" Type="INTEGER">80</Param>
		<Param Name="Decomb Enable" Type="ENUM">DECOMBING_DISABLED</Param>
		<Param Name="Decomb Threshold" Type="INTEGER">0</Param>
	</Parameters>

</PluginInclude>
